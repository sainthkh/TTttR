96\'3F. \u46372\'3F\u54980\'3F3:15. \u50864\'3F\u47532\'3F\u44032\'3F \u47924\'3F\u51648\'3F\u54616\'3F\u45796\'3F\u47732\'3F, \u49457\'3F\u44221\'3F\u51008\'3F \u50864\'3F\u47532\'3F\u47484\'3F \u44032\'3F\u47476\'3F\u52841\'3F\u45768\'3F\u45796\'3F. \u50864\'3F\u47532\'3F\u44032\'3F \u44600\'3F\u51012\'3F \u51075\'3F\u50632\'3F\u45796\'3F\u47732\'3F, \u49457\'3F\u44221\'3F\u51008\'3F \u50864\'3F\u47532\'3F\u47484\'3F \u51665\'3F\u51004\'3F\u47196\'3F \u51064\'3F\u46020\'3F\u54633\'3F\u45768\'3F\u45796\'3F. \u50864\'3F\u47532\'3F\u44032\'3F \u47581\'3F\u44032\'3F\u51276\'3F\u45796\'3F\u47732\'3F, \u49457\'3F\u44221\'3F\u51008\'3F \u50864\'3F\u47532\'3F\u47484\'3F \u49352\'3F\u47213\'3F\u44172\'3F \u47564\'3F\u46308\'3F\u50612\'3F\u51469\'3F\u45768\'3F\u45796\'3F. \u50864\'3F\u47532\'3F\u44032\'3F \u44540\'3F\u49900\'3F \u49549\'3F\u50640\'3F \u51080\'3F\u45796\'3F\u47732\'3F \u49457\'3F\u44221\'3F\u51008\'3F \u50864\'3F\u47532\'3F\u47484\'3F \u50948\'3F\u47196\'3F\u54633\'3F\u45768\'3F\u45796\'3F. \u50864\'3F\u47532\'3F\u44032\'3F \u44172\'3F\u51004\'3F\u47484\'3F \u46412\'3F, \u50864\'3F\u47532\'3F\u47484\'3F \u49549\'3F\u55176\'3F \u50880\'3F\u51649\'3F\u51060\'3F\u44172\'3F \u54633\'3F\u45768\'3F\u45796\'3F. \u50864\'3F\u47532\'3F\u51032\'3F \u49888\'3F\u50521\'3F\u51060\'3F \u52264\'3F\u44032\'3F\u50872\'3F \u46412\'3F \u50864\'3F\u47532\'3F\u47484\'3F \u48520\'3F\u53440\'3F\u50724\'3F\u47476\'3F\u44172\'3F \u54633\'3F\u45768\'3F\u45796\'3F. (\u49457\'3F\u44221\'3F\u50640\'3F \u48169\'3F\u54693\'3F\u51060\'3F \u51452\'3F\u50612\'3F\u51276\'3F\u51004\'3F\u45768\'3F) \u49457\'3F\u44221\'3F\u51012\'3F \u51105\'3F\u44256\'3F \u51069\'3F\u51021\'3F\u49884\'3F\u45796\'3F. \u51105\'3F\u44256\'3F \u51069\'3F\u51021\'3F\u49884\'3F\u45796\'3F(Tolle, lege; Tolle, lege). \u51060\'3F \u47928\'3F\u51109\'3F\u51008\'3F \u52488\'3F\u51088\'3F\u50672\'3F\u51201\'3F\u51064\'3F \u47785\'3F\u49548\'3F\u47532\'3F\u47196\'3F \u50612\'3F\u44144\'3F\u49828\'3F\u54004\'3F\u50640\'3F\u44172\'3F \u51060\'3F\u50556\'3F\u44592\'3F\u46104\'3F\u50632\'3F\u49845\'3F\u45768\'3F\u45796\'3F. \u8220\'93\u51228\'3F \u47568\'3F\u51012\'3F \u48127\'3F\u50612\'3F\u51452\'3F\u49884\'3F\u44600\'3F \u48148\'3F\u46989\'3F\u45768\'3F\u45796\'3F. \u49457\'3F\u44221\'3F\u50640\'3F \u51080\'3F\u45716\'3F \u44163\'3F\u51008\'3F \u44536\'3F \u50612\'3F\u46500\'3F \u44163\'3F\u51060\'3F\u46972\'3F\u46020\'3F \u45796\'3F \u44256\'3F\u44480\'3F\u54616\'3F\u44256\'3F, divine\u54633\'3F\u45768\'3F\u45796\'3F. \u44536\'3F\u44275\'3F\u50640\'3F\u45716\'3F \u52280\'3F\u46108\'3F \u51652\'3F\u47532\'3F\u44032\'3F \u51080\'3F\u44256\'3F, \u49324\'3F\u46988\'3F\u51032\'3F \u47560\'3F\u51020\'3F\u51012\'3F \u49352\'3F\u47213\'3F\u44172\'3F \u54616\'3F\u45716\'3F\u45936\'3F \u44032\'3F\u51109\'3F \u51201\'3F\u54633\'3F\u54620\'3F \u44368\'3F\u47532\'3F\u44032\'3F \u51080\'3F\u44256\'3F, \u51221\'3F\u47568\'3F \u47566\'3F\u51060\'3F tempered\u46104\'3F\u50612\'3F \u51080\'3F\u50612\'3F\u49436\'3F \u51652\'3F\u51221\'3F\u54620\'3F \u51333\'3F\u44368\'3F\u44032\'3F \u50836\'3F\u44396\'3F\u54616\'3F\u46319\'3F\u51060\'3F \u44536\'3F\u44032\'3F devout and pious\u54620\'3F \u47560\'3F\u51020\'3F\u51012\'3F \u44032\'3F\u51648\'3F\u44256\'3F \u45208\'3F\u50500\'3F\u50772\'3F\u45796\'3F\u47732\'3F, \u45572\'3F\u44396\'3F\u45208\'3F \u44536\'3F\u44163\'3F\u51004\'3F\u47196\'3F\u48512\'3F\u53552\'3F \u51088\'3F\u44592\'3F\u50640\'3F\u44172\'3F \u52649\'3F\u48516\'3F\u54620\'3F \u44163\'3F\u51012\'3F \u45132\'3F\u50612\'3F \u50732\'3F \u49688\'3F \u51080\'3F\u49845\'3F\u45768\'3F\u45796\'3F.\u8221\'94\u46972\'3F\u44256\'3F \u48169\'3F\u44552\'3F \u50616\'3F\u44553\'3F\u54620\'3F \u50612\'3F\u44144\'3F\u49828\'3F\u54004\'3F\u51060\'3F \u51060\'3F\u50556\'3F\u44592\'3F\u54664\'3F\u49845\'3F\u45768\'3F\u45796\'3F. \u44536\'3F\u47000\'3F\u49436\'3F \u50612\'3F\u44144\'3F\u49828\'3F\u54004\'3F\u44284\'3F \u51228\'3F\u47212\'3F\u51008\'3F \u8220\'93\u49457\'3F\u44221\'3F\u51012\'3F \u49324\'3F\u46993\'3F\u54616\'3F\u49901\'3F\u49884\'3F\u50724\'3F. \u44536\'3F\u47084\'3F\u47732\'3F \u51648\'3F\u54812\'3F\u44032\'3F \u45817\'3F\u49888\'3F\u51012\'3F \u49324\'3F\u46993\'3F\u54624\'3F \u44163\'3F\u51077\'3F\u45768\'3F\u45796\'3F.(Ama scripturas, et amabit te spientia etc)\u8221\'94\u46972\'3F\u44256\'3F \u51060\'3F\u50556\'3F\u44592\'3F\u54664\'3F\u49845\'3F\u45768\'3F\u45796\'3F. Cyril\u51008\'3F Julian\u50640\'3F\u44172\'3F \u8220\'93\u49457\'3F\u44221\'3F \u49549\'3F\u50640\'3F\u49436\'3F \u51088\'3F\u46976\'3F \u50500\'3F\u51060\'3F\u44032\'3F \u44032\'3F\u51109\'3F \u49888\'3F\u50521\'3F\u49900\'3F\u51008\'3F \u47932\'3F\u47200\'3F \u45796\'3F\u47480\'3F \u47732\'3F\u50640\'3F\u49436\'3F\u46020\'3F \u44032\'3F\u51109\'3F \u46832\'3F\u50612\'3F\u45212\'3F \u50500\'3F\u51060\'3F\u44032\'3F \u46108\'3F\u45796\'3F.\u8221\'94\u46972\'3F\u44256\'3F \u51060\'3F\u50556\'3F\u44592\'3F\u54664\'3F\u49845\'3F\u45768\'3F\u45796\'3F. \u48127\'3F\u50612\'3F\u51256\'3F \u50728\'3F \u44163\'3F\u46308\'3F, \u44536\'3F\u47532\'3F\u44256\'3F \u49892\'3F\u54665\'3F\u46104\'3F\u44256\'3F \u48148\'3F\u46972\'3F\u50724\'3F\u45912\'3F \u44163\'3F\u46308\'3F \u51473\'3F \u44536\'3F \u50612\'3F\u46500\'3F \u44163\'3F\u51060\'3F\u46972\'3F\u46020\'3F \u49457\'3F\u44221\'3F\u50640\'3F \u51080\'3F\u45796\'3F\u45716\'3F \u44163\'3F\u51012\'3F \u49457\'3F\u44221\'3F\u51032\'3F \u49436\'3F\u45320\'3F\u44032\'3F\u51648\'3F \u50857\'3F\u47168\'3F\u47484\'3F \u46308\'3F\u50612\'3F\u49436\'3F \u47568\'3F\u54624\'3F \u54596\'3F\u50836\'3F\u44032\'3F \u51080\'3F\u44192\'3F\u49845\'3F\u45768\'3F\u44620\'3F? \u46608\'3F\u45716\'3F \u44536\'3F\u47532\'3F\u49828\'3F\u46020\'3F\u51032\'3F \u49884\'3F\u45824\'3F\u47196\'3F\u48512\'3F\u53552\'3F \u51648\'3F\u44552\'3F\u44620\'3F\u51648\'3F \u44368\'3F\u48512\'3F\u46972\'3F \u48520\'3F\u47540\'3F\u47564\'3F\u54620\'3F \u48516\'3F\u46308\'3F\u44760\'3F\u49436\'3F \u49457\'3F\u44221\'3F\u51032\'3F \u50756\'3F\u48317\'3F\u54632\'3F\u44284\'3F \u48512\'3F\u50836\'3F\u54632\'3F\u50640\'3F \u45824\'3F\u54644\'3F \u48708\'3F\u49847\'3F\u54616\'3F\u44172\'3F \u51060\'3F\u50556\'3F\u44592\'3F\u54616\'3F\u44256\'3F \u51080\'3F\u45796\'3F\u44256\'3F, \u49436\'3F\u45320\'3F\u44060\'3F\u51032\'3F \u47928\'3F\u51109\'3F\u46308\'3F\u51012\'3F \u50616\'3F\u44553\'3F\u54644\'3F\u50556\'3F \u47924\'3F\u50631\'3F\u51012\'3F \u54616\'3F\u44192\'3F\u49845\'3F\u45768\'3F\u44620\'3F? \u8220\'93\u45208\'3F\u45716\'3F \u49457\'3F\u44221\'3F\u51032\'3F \u52649\'3F\u47564\'3F\u54632\'3F\u51012\'3F \u49324\'3F\u46993\'3F\u54620\'3F\u45796\'3F.\u8221\'94\u46972\'3F\u44256\'3F Tertullian\u51060\'3F Hermogenes\u50640\'3F\u44172\'3F \u51060\'3F\u50556\'3F\u44592\'3F\u54664\'3F\u49845\'3F\u45768\'3F\u45796\'3F. \u44536\'3F\u45716\'3F \u46608\'3F\u54620\'3F \u48708\'3F\u49847\'3F\u54620\'3F \u51333\'3F\u47448\'3F\u51032\'3F \u51060\'3F\u45800\'3F\u51064\'3F Apelles\u50640\'3F\u44172\'3F \u8220\'93\u45208\'3F\u45716\'3F \u44536\'3F\u45824\'3F\u44032\'3F \u49457\'3F\u44221\'3F\u50630\'3F\u51060\'3F \u46308\'3F\u50612\'3F\u50724\'3F\u44144\'3F\u45208\'3F (\u54841\'3F\u51008\'3F \u44208\'3F\u47200\'3F\u51648\'3F\u51008\'3F) \u45817\'3F\u49888\'3F\u51032\'3F \u44163\'3F\u46308\'3F(\u47672\'3F\u47532\'3F \u54841\'3F\u51008\'3F \u51200\'3F\u51089\'3F\u47932\'3F, \u46608\'3F\u45716\'3F \u46168\'3F \u45796\'3F)\u50640\'3F \u46041\'3F\u51032\'3F\u54624\'3F \u49688\'3F \u50630\'3F\u49845\'3F\u45768\'3F\u45796\'3F\u8221\'94\u46972\'3F\u44256\'3F \u51060\'3F\u50556\'3F\u44592\'3F\u54664\'3F\u49845\'3F\u45768\'3F\u45796\'3F. \u49692\'3F\u44368\'3F\u51088\'3F \u51200\'3F\u49828\'3F\u54004\'3F\u51008\'3F before him, \u8220\'93\u50864\'3F\u47532\'3F\u45716\'3F \u54616\'3F\u45208\'3F\u45784\'3F\u50640\'3F \u45824\'3F\u54620\'3F \u44163\'3F\u51060\'3F\u45208\'3F \u50739\'3F\u51008\'3F \u49888\'3F\u50521\'3F\u50640\'3F \u45824\'3F\u54644\'3F \u48176\'3F\u50864\'3F\u45716\'3F \u44163\'3F\u51008\'3F \u50724\'3F\u51649\'3F divine \u50689\'3F\u44048\'3F\u51012\'3F \u53685\'3F\u54644\'3F \u50864\'3F\u47532\'3F\u50640\'3F\u44172\'3F \u44032\'3F\u47476\'3F\u52432\'3F \u51452\'3F\u49884\'3F\u45716\'3F \u45824\'3F\u50616\'3F\u51088\'3F\u46308\'3F\u51032\'3F \u44544\'3F\u46308\'3F\u51012\'3F \u53685\'3F\u54644\'3F \u50508\'3F\u44172\'3F \u46108\'3F\u44163\'3F\u51012\'3F \u51228\'3F\u50808\'3F\u54616\'3F\u44256\'3F\u45716\'3F \u54633\'3F\u48277\'3F\u51201\'3F\u51060\'3F\u51648\'3F \u50506\'3F\u45796\'3F(\u54841\'3F\u51008\'3F \u48520\'3F\u44032\'3F\u45733\'3F\u54616\'3F\u45796\'3F).\u8221\'94 \u44536\'3F\u47000\'3F\u49436\'3F Tertullian \u51060\'3F\u54980\'3F\u51032\'3F Basil\u51008\'3F \u8220\'93\u51201\'3F\u54784\'3F \u51080\'3F\u45716\'3F \u44163\'3F\u46308\'3F \u51473\'3F \u50612\'3F\u46500\'3F \u44163\'3F\u51060\'3F\u46972\'3F\u46020\'3F \u44144\'3F\u48512\'3F\u54616\'3F\u44144\'3F\u45208\'3F, \u51201\'3F\u54784\'3F \u51080\'3F\u51648\'3F \u50506\'3F\u51008\'3F \u44163\'3F\u51012\'3F \u44032\'3F\u51256\'3F \u50724\'3F\u45716\'3F \u44163\'3F\u51008\'3F \u48127\'3F\u51020\'3F\u50640\'3F\u49436\'3F \u49900\'3F\u44033\'3F\u54616\'3F\u44172\'3F \u46504\'3F\u50612\'3F\u51256\'3F \u45208\'3F\u44032\'3F\u45716\'3F \u44163\'3F\u51060\'3F\u47728\'3F, \u44032\'3F\u51221\'3F\u51032\'3F \u50724\'3F\u47448\'3F\u51060\'3F\u45796\'3F"\u46972\'3F\u44256\'3F \u54664\'3F\u49845\'3F\u45768\'3F\u45796\'3F. \u50864\'3F\u47532\'3F\u45716\'3F \u50696\'3F\u47336\'3F\u49332\'3F\u47128\'3F\u51032\'3F Cyril\u51064\'3F Basil\u51060\'3F \u44536\'3F\u51032\'3F Cataches 4\u51109\'3F\u51060\'3F\u45208\'3F \u51228\'3F\u47212\'3F\u51060\'3F Helvidius\u50640\'3F\u44172\'3F \u51201\'3F\u51008\'3F \u44163\'3F, \u50612\'3F\u44144\'3F\u49828\'3F\u54004\'3F\u51060\'3F Petilian\u51032\'3F \u54200\'3F\u51648\'3F\u46308\'3F\u50640\'3F \u45813\'3F\u51109\'3F\u54616\'3F\u47728\'3F \u50420\'3F \u44544\'3F\u51060\'3F\u45208\'3F \u46608\'3F \u45796\'3F\u47480\'3F \u50668\'3F\u47084\'3F \u44275\'3F\u50640\'3F\u49436\'3F \u44536\'3F\u44032\'3F \u51201\'3F\u51008\'3F \u44163\'3F\u46308\'3F\u50640\'3F \u45824\'3F\u54644\'3F\u49436\'3F \u49373\'3F\u47029\'3F\u54633\'3F\u45768\'3F\u45796\'3F. \u50864\'3F\u47532\'3F\u44032\'3F \u54980\'3F\u45824\'3F\u51032\'3F \u44368\'3F\u48512\'3F\u46308\'3F\u44620\'3F\u51648\'3F \u45236\'3F\u47140\'3F\u44032\'3F\u45716\'3F \u44163\'3F\u51008\'3F \u49340\'3F\u44032\'3F\u47140\'3F \u54633\'3F\u45768\'3F\u45796\'3F. \u51060\'3F\u45716\'3F \u50864\'3F\u47532\'3F\u44032\'3F \u46021\'3F\u51088\'3F\u46308\'3F\u51012\'3F \u51648\'3F\u52824\'3F\u44172\'3F \u54616\'3F\u44256\'3F \u49910\'3F\u51648\'3F \u50506\'3F\u44592\'3F \u46412\'3F\u47928\'3F\u51077\'3F\u45768\'3F\u45796\'3F. \u49457\'3F\u44221\'3F\u51060\'3F \u47588\'3F\u50864\'3F \u52649\'3F\u47564\'3F\u54616\'3F\u44256\'3F \u47588\'3F\u50864\'3F \u50756\'3F\u48317\'3F\u54620\'3F \u44163\'3F\u51076\'3F\u51012\'3F \u50508\'3F\u44172\'3F \u46104\'3F\u50632\'3F\u51004\'3F\u45768\'3F, \u50864\'3F\u47532\'3F\u44032\'3F \u49457\'3F\u44221\'3F\u51012\'3F \u44277\'3F\u48512\'3F\u54616\'3F\u51648\'3F \u50506\'3F\u45716\'3F\u45796\'3F\u47732\'3F \u50864\'3F\u47532\'3F\u51032\'3F \u47924\'3F\u51648\'3F\u54632\'3F\u51012\'3F \u50612\'3F\u52236\'3F \u51221\'3F\u45817\'3F\u54868\'3F\u54624\'3F \u49688\'3F \u51080\'3F\u51012\'3F\u44620\'3F\u50836\'3F? \u50864\'3F\u47532\'3F\u44032\'3F \u49457\'3F\u44221\'3F\u50640\'3F\u49436\'3F \u47564\'3F\u51313\'3F\u54632\'3F\u51012\'3F \u50619\'3F\u51648\'3F \u47803\'3F\u54620\'3F\u45796\'3F\u47732\'3F, \u50864\'3F\u47532\'3F\u44032\'3F \u50612\'3F\u46523\'3F\u44172\'3F \u54840\'3F\u44592\'3F\u49900\'3F\u51012\'3F \u48320\'3F\u47749\'3F\u54624\'3F \u49688\'3F \u51080\'3F\u51012\'3F\u44620\'3F\u50836\'3F? \u49324\'3F\u46988\'3F\u46308\'3F\u51008\'3F [\u44536\'3F\u47532\'3F\u49828\'3F\u50612\'3F \u45800\'3F\u50612\'3F]\u50640\'3F \u45824\'3F\u54644\'3F \u44536\'3F\u44163\'3F\u50640\'3F \u50620\'3F\u47560\'3F\u45208\'3F \u51339\'3F\u44256\'3F \u47691\'3F\u51652\'3F \u44163\'3F\u46308\'3F\u51060\'3F \u45804\'3F\u47140\'3F \u51080\'3F\u45716\'3F\u51648\'3F\u50640\'3F \u45824\'3F\u54644\'3F, \u46608\'3F \u54788\'3F\u51088\'3F\u51032\'3F \u46028\'3F\u50640\'3F \u45824\'3F\u54644\'3F\u49436\'3F\u46020\'3F \u44536\'3F\u44163\'3F\u51060\'3F \u44396\'3F\u47532\'3F\u47484\'3F \u44552\'3F\u51004\'3F\u47196\'3F \u48148\'3F\u44984\'3F\u45716\'3F\u51648\'3F\u50640\'3F \u45824\'3F\u54644\'3F\u49436\'3F, \u47673\'3F\u45716\'3F\u45936\'3F \u54596\'3F\u50836\'3F\u54620\'3F \u47784\'3F\u46304\'3F \u44163\'3F\u51060\'3F \u51080\'3F\u45716\'3F Cornucopia\u45208\'3F \u48337\'3F\u46308\'3F\u50640\'3F \u51339\'3F\u45796\'3F\u45716\'3F Panacea\u46972\'3F\u45716\'3F \u50557\'3F\u52488\'3F\u45208\'3F \u47784\'3F\u46304\'3F \u45208\'3F\u49244\'3F \u49373\'3F\u44033\'3F\u51012\'3F \u47932\'3F\u47532\'3F\u52432\'3F \u51456\'3F\u45796\'3F\u45716\'3F Catholicon\u51060\'3F\u46972\'3F\u45716\'3F \u50557\'3F, \u44536\'3F \u50612\'3F\u46500\'3F \u47924\'3F\u44592\'3F\u47196\'3F \u52244\'3F\u47532\'3F\u45716\'3F \u44163\'3F\u51060\'3F\u45208\'3F \u44277\'3F\u44201\'3F\u50640\'3F\u46020\'3F \u50756\'3F\u48317\'3F\u55176\'3F \u48169\'3F\u50612\'3F\u54624\'3F \u49688\'3F \u51080\'3F\u45716\'3F Vulcan\u51032\'3F \u44049\'3F\u50743\'3F \u46321\'3F\u50640\'3F \u45824\'3F\u54644\'3F \u51088\'3F\u51452\'3F \u51060\'3F\u50556\'3F\u44592\'3F\u54633\'3F\u45768\'3F\u45796\'3F. Well, \u44536\'3F\u46308\'3F\u51008\'3F \u51060\'3F \u47784\'3F\u46304\'3F \u44163\'3F\u51012\'3F \u50977\'3F\u52404\'3F\u51032\'3F \u49888\'3F\u51012\'3F \u50948\'3F\u54644\'3F \u51096\'3F\u47803\'3F\u46104\'3F\u44144\'3F\u45208\'3F \u54747\'3F\u46104\'3F\u44172\'3F \u50689\'3F\u44305\'3F\u51012\'3F \u46028\'3F\u47532\'3F\u51648\'3F\u47564\'3F \u50864\'3F\u47532\'3F\u45716\'3F \u51221\'3F\u51032\'3F\u47213\'3F\u44172\'3F \u44536\'3F\u47532\'3F\u44256\'3F \u47784\'3F\u46304\'3F \u49688\'3F\u45800\'3F\u51012\'3F \u44054\'3F\u52656\'3F\u49436\'3F \u50689\'3F\u51201\'3F\u51064\'3F \u51012\'3F \u50948\'3F\u54644\'3F \u49457\'3F\u44221\'3F\u50640\'3F \u50689\'3F\u44305\'3F\u51012\'3F \u46028\'3F\u47549\'3F\u45768\'3F\u45796\'3F. \u49457\'3F\u44221\'3F\u51008\'3F \u48169\'3F\u50612\'3F\u44396\'3F\u51068\'3F \u49104\'3F\u47564\'3F \u50500\'3F\u45768\'3F\u46972\'3F \u47784\'3F\u46304\'3F \u44163\'3F\u51060\'3F \u44054\'3F\u52656\'3F\u51652\'3F \u48169\'3F\u50612\'3F\u50752\'3F \u44277\'3F\u44201\'3F\u51060\'3F \u46168\'3F \u45796\'3F \u44032\'3F\u45733\'3F\u54620\'3F \u47924\'3F\u44592\'3F\u46308\'3F\u51060\'3F\u44592\'3F\u46020\'3F \u54633\'3F\u45768\'3F\u45796\'3F. \u51060\'3F\u44163\'3F\u51004\'3F\u47196\'3F \u50864\'3F\u47532\'3F\u45716\'3F \u50864\'3F\u47532\'3F \u51088\'3F\u49888\'3F\u51012\'3F \u44396\'3F\u50896\'3F\u54616\'3F\u44592\'3F\u46020\'3F \u54616\'3F\u44256\'3F \u50896\'3F\u49688\'3F\u47484\'3F \u46020\'3F\u47581\'3F\u44032\'3F\u44172\'3F \u54616\'3F\u44592\'3F\u46020\'3F \u54633\'3F\u45768\'3F\u45796\'3F. }}