'3F\u51064\'3F\u46020\'3F, \u44536\'3F\u47532\'3F\u44256\'3F \u50728\'3F \u46405\'3F\u50640\'3F \u55145\'3F\u50612\'3F\u51652\'3F \u47784\'3F\u46304\'3F \u49324\'3F\u46988\'3F\u46308\'3F\u46020\'3F }{\f0\fs24\b0\i0 \loch\af0\hich\af0\dbch\af0\uc1\u44536\'3F\u48516\'3F\u51032\'3F \u54588\'3F\u47484\'3F \u53685\'3F\u54644\'3F \u48127\'3F\u51020\'3F\u51004\'3F\u47196\'3F \u54868\'3F\u47785\'3F\u54616\'3F\u44172\'3F \u46104\'3F\u46020\'3F\u47197\'3F \u54616\'3F\u45208\'3F\u45784\'3F\u44760\'3F\u49436\'3F \u51228\'3F\u49324\'3F\u51109\'3F \u51649\'3F\u48516\'3F\u51012\'3F \u44536\'3F\u48516\'3F\u44760\'3F \u47585\'3F\u44592\'3F\u49512\'3F\u49845\'3F\u45768\'3F\u45796\'3F. \u46384\'3F\u46972\'3F\u49436\'3F \u51096\'3F \u49332\'3F\u54196\'3F \u48372\'3F\u49884\'3F\u44592\'3F \u48148\'3F\u46989\'3F\u45768\'3F\u45796\'3F. \u44536\'3F\u47532\'3F\u49828\'3F \u50773\'3F\u51032\'3F \u50689\'3F\u51012\'3F \u44048\'3F\u46041\'3F\u49884\'3F\u53412\'3F\u45716\'3F \u44163\'3F\u51012\'3F \u51452\'3F\u45784\'3F\u44760\'3F\u49436\'3F \u44592\'3F\u49240\'3F\u44172\'3F \u50668\'3F\u44592\'3F\u49884\'3F\u44256\'3F(\u50668\'3F\u44592\'3F\u49436\'3F \u44536\'3F\u47532\'3F\u49828\'3F\u46976\'3F descent\u50752\'3F \u50616\'3F\u50612\'3F\u47484\'3F \u45796\'3F \u51032\'3F\u48120\'3F\u54633\'3F\u45768\'3F\u45796\'3F), \u49900\'3F\u51648\'3F\u50612\'3F \u51060\'3F\u51665\'3F\u53944\'3F\u51032\'3F \u50773\'3F Ptolemy Philadelph\u51032\'3F \u50689\'3F\u51312\'3F\u52264\'3F \u44048\'3F\u46041\'3F\u49884\'3F\u53412\'3F\u49492\'3F\u49436\'3F \u54616\'3F\u45208\'3F\u45784\'3F\u51032\'3F \u52293\'3F\u51012\'3F \u55176\'3F\u48652\'3F\u47532\'3F\u50612\'3F\u50640\'3F\u49436\'3F \u44536\'3F\u47532\'3F\u49828\'3F\u50612\'3F\u47196\'3F \u50734\'3F\u44596\'3F \u44163\'3F\u51012\'3F \u44396\'3F\u54624\'3F \u49688\'3F \u51080\'3F\u44172\'3F \u54616\'3F\u49512\'3F\u49845\'3F\u45768\'3F\u45796\'3F. \u51060\'3F\u44163\'3F\u51060\'3F \u55124\'3F\u55176\'3F \u51060\'3F\u50556\'3F\u44592\'3F\u54616\'3F\u45716\'3F \u52832\'3F\u49901\'3F\u51064\'3F\u50669\'3F\u51060\'3F\u47728\'3F, \u51060\'3F\u44163\'3F\u51008\'3F \u51060\'3F\u48169\'3F\u51064\'3F\u46308\'3F \u51473\'3F\u50640\'3F \u50724\'3F\u49884\'3F\u45716\'3F \u50864\'3F\u47532\'3F \u44396\'3F\u50896\'3F\u51088\'3F\u51032\'3F \u44600\'3F\u51012\'3F \u47560\'3F\u52824\'3F \u52840\'3F\u47168\'3F\u51088\'3F \u50836\'3F\u54620\'3F\u51060\'3F \u50976\'3F\u45824\'3F\u51064\'3F \u49324\'3F\u51060\'3F\u50640\'3F\u49436\'3F \u47568\'3F\u47196\'3F \u44536\'3F\u47111\'3F\u44172\'3F \u54664\'3F\u45912\'3F \u44163\'3F\u52376\'3F\u47100\'3F \u44544\'3F\u47196\'3F \u46108\'3F \u49444\'3F\u44368\'3F(written preaching)\u47196\'3F \u50696\'3F\u48708\'3F\u54664\'3F\u49845\'3F\u45768\'3F\u45796\'3F. Grecian\u46308\'3F\u51008\'3F \u48176\'3F\u50864\'3F\u45716\'3F \u44163\'3F\u51012\'3F \u47924\'3F\u52377\'3F \u51339\'3F\u50500\'3F\u54664\'3F\u44592\'3F \u46412\'3F\u47928\'3F\u50640\'3F \u44032\'3F\u52824\'3F \u51080\'3F\u45716\'3F \u52293\'3F\u46308\'3F\u51060\'3F \u50773\'3F\u51032\'3F \u46020\'3F\u49436\'3F\u44288\'3F\u46308\'3F(libraries)\u50640\'3F \u51109\'3F\u49885\'3F\u54408\'3F\u51004\'3F\u47196\'3F\u47564\'3F \u45459\'3F\u50668\'3F \u51080\'3F\u44592\'3F\u47484\'3F \u50896\'3F\u52824\'3F \u50506\'3F\u50520\'3F\u44256\'3F, \u44536\'3F\u46308\'3F\u51032\'3F \u49688\'3F\u47566\'3F\u51008\'3F \u51333\'3F\u46308\'3F\u44284\'3F \u51456\'3F\u48708\'3F\u46108\'3F \u49436\'3F\u44592\'3F\u44288\'3F\u46308\'3F\u51012\'3F \u49884\'3F\u53020\'3F \u44536\'3F \u52293\'3F\u46308\'3F\u51012\'3F \u51204\'3F\u48512\'3F \u50734\'3F\u44200\'3F \u51201\'3F\u44172\'3F \u54616\'3F\u50688\'3F\u44256\'3F, \u51060\'3F\u47484\'3F \u53685\'3F\u54644\'3F \u44536\'3F \u52293\'3F\u46308\'3F\u51060\'3F \u45328\'3F\u47532\'3F \u54140\'3F\u51256\'3F \u55124\'3F\u54644\'3F\u51648\'3F\u44172\'3F \u46104\'3F\u50632\'3F\u49845\'3F\u45768\'3F\u45796\'3F. \u45796\'3F\u49884\'3F \u47568\'3F\u54644\'3F, \u44536\'3F\u47532\'3F\u49828\'3F\u50612\'3F\u45716\'3F \u50500\'3F\u49884\'3F\u50500\'3F\u50640\'3F \u49332\'3F\u44256\'3F \u51080\'3F\u45716\'3F \u44144\'3F\u51032\'3F \u47784\'3F\u46304\'3F \u49324\'3F\u46988\'3F\u46308\'3F\u50640\'3F\u44172\'3F \u51096\'3F \u50508\'3F\u47140\'3F\u51648\'3F\u44256\'3F, \u51061\'3F\u49689\'3F\u54620\'3F \u50616\'3F\u50612\'3F\u50688\'3F\u45716\'3F\u45936\'3F, Grecian\u46308\'3F\u51060\'3F \u51068\'3F\u51004\'3F\u53040\'3F\u45912\'3F \u49688\'3F\u50630\'3F\u51060\'3F \u47566\'3F\u51008\'3F \u51221\'3F\u48373\'3F \u51204\'3F\u51137\'3F\u46308\'3F\u44284\'3F \u44536\'3F\u46308\'3F\u51060\'3F \u47564\'3F\u46308\'3F\u50612\'3F\u45240\'3F \u49688\'3F\u47566\'3F\u51008\'3F \u49885\'3F\u48124\'3F\u51648\'3F\u46308\'3F, \u44536\'3F\u47532\'3F\u44256\'3F \u44536\'3F\u44275\'3F\u51004\'3F\u47196\'3F \u48372\'3F\u45240\'3F \u49688\'3F\u47566\'3F\u51008\'3F \u44400\'3F\u51064\'3F\u46308\'3F\u51060\'3F \u51080\'3F\u50632\'3F\u44592\'3F \u46412\'3F\u47928\'3F\u51077\'3F\u45768\'3F\u45796\'3F. \u44057\'3F\u51008\'3F \u51060\'3F\u50976\'3F\u47196\'3F \u47566\'3F\u51008\'3F \u50976\'3F\u47101\'3F \u51648\'3F\u48169\'3F\u50640\'3F\u49436\'3F, \u44536\'3F\u47532\'3F\u44256\'3F \u50500\'3F\u54532\'3F\u47532\'3F\u52852\'3F \u51648\'3F\u50669\'3F\u50640\'3F\u49436\'3F \u47566\'3F\u51008\'3F \u49324\'3F\u46988\'3F\u46308\'3F\u51060\'3F \u51060\'3F \u50616\'3F\u50612\'3F\u47484\'3F \u51060\'3F\u54644\'3F\u54664\'3F\u49845\'3F\u45768\'3F\u45796\'3F. }}