\pard\plain \fi720\ltrch\loch {\f0\fs24\b0\i0 \loch\af0\hich\af0\dbch\af0\uc1\u49457\'3F\u44221\'3F\u51008\'3F \u50557\'3F\u52488\'3F\u44032\'3F \u50500\'3F\u45768\'3F\u44256\'3F, \u45208\'3F\u47924\'3F\u51060\'3F\u47728\'3F, \u54841\'3F\u51008\'3F \u45908\'3F \u45208\'3F\u50500\'3F\u44032\'3F \u49373\'3F\u47749\'3F\u45208\'3F\u47924\'3F\u46308\'3F\u51032\'3F \u45209\'3F\u50896\'3F \u44536\'3F \u51088\'3F\u52404\'3F(a whole paradise of trees of life)\u46972\'3F\u44256\'3F \u54624\'3F \u49688\'3F \u51080\'3F\u45716\'3F\u45936\'3F, \u44536\'3F\u44275\'3F\u50640\'3F\u49436\'3F \u47588\'3F\u45804\'3F \u45208\'3F\u50724\'3F\u45716\'3F \u50676\'3F\u47588\'3F\u47484\'3F \u50864\'3F\u47532\'3F\u45716\'3F \u47673\'3F\u51012\'3F\u44144\'3F\u47532\'3F\u47196\'3F, \u51086\'3F\u49324\'3F\u44480\'3F\u45716\'3F \u50557\'3F\u51004\'3F\u47196\'3F \u49324\'3F\u50857\'3F\u54633\'3F\u45768\'3F\u45796\'3F. \u49457\'3F\u44221\'3F\u51008\'3F \u50724\'3F\u51649\'3F \u44592\'3F\u50613\'3F\u54616\'3F\u44592\'3F \u50948\'3F\u54644\'3F \u54841\'3F\u51008\'3F \u54620\'3F \u45180\'3F \u49885\'3F\u49324\'3F\u47484\'3F \u50948\'3F\u54644\'3F \u47564\'3F\u46308\'3F\u50612\'3F\u51652\'3F \u47564\'3F\u45208\'3F \u54637\'3F\u50500\'3F\u47532\'3F \u54620\'3F \u44060\'3F\u45208\'3F \u44592\'3F\u47492\'3F \u54620\'3F \u48337\'3F\u51060\'3F \u50500\'3F\u45768\'3F\u46972\'3F \u50728\'3F \u48177\'3F\u49457\'3F(whole host)\u50640\'3F\u44172\'3F \u52649\'3F\u48516\'3F\u54620\'3F \u54616\'3F\u45720\'3F\u51032\'3F \u50577\'3F\u49885\'3F\u51032\'3F \u49548\'3F\u45208\'3F\u44592\'3F \u44057\'3F\u50500\'3F\u49436\'3F \u51221\'3F\u47568\'3F \u45824\'3F\u45800\'3F\u54616\'3F\u45796\'3F\u46972\'3F\u44256\'3F \u47568\'3F\u54624\'3F \u49688\'3F \u50630\'3F\u51012\'3F \u51221\'3F\u46020\'3F\u47196\'3F \u47566\'3F\u49845\'3F\u45768\'3F\u45796\'3F(}{\f0\fs24\b0\i0 be it never so great).}{\f0\fs24\b0\i0  \loch\af0\hich\af0\dbch\af0\uc1\u46608\'3F\u54620\'3F \u49457\'3F\u44221\'3F\u51008\'3F \u44592\'3F\u47492\'3F \u44536\'3F\u47495\'3F\u51004\'3F\u47196\'3F \u44032\'3F\u46301\'3F\u54620\'3F \u52285\'3F\u44256\'3F \u44057\'3F\u50500\'3F\u49436\'3F \u44536\'3F\u44275\'3F\u50640\'3F\u49436\'3F \u50864\'3F\u47532\'3F\u50640\'3F\u44172\'3F \u54596\'3F\u50836\'3F\u54620\'3F \u47784\'3F\u46304\'3F \u44163\'3F\u51060\'3F \u52292\'3F\u50892\'3F\u51648\'3F\u47728\'3F, \u50864\'3F\u47532\'3F\u51032\'3F \u47784\'3F\u46304\'3F \u48730\'3F\u51060\'3F \u44058\'3F\u50500\'3F\u51665\'3F\u45768\'3F\u45796\'3F. \u44036\'3F\u47029\'3F\u55176\'3F \u47568\'3F\u54644\'3F, \u49457\'3F\u44221\'3F\u51008\'3F fenow\u54620\'3F \u51204\'3F\u53685\'3F\u51012\'3F \u47932\'3F\u47532\'3F\u52832\'3F \u49688\'3F \u51080\'3F\u45716\'3F \u47800\'3F\u50640\'3F \u51339\'3F\u51008\'3F \u51020\'3F\u49885\'3F\u51060\'3F \u45812\'3F\u44596\'3F panary\u51060\'3F\u47728\'3F, (Basil\u51060\'3F \u47568\'3F\u54620\'3F \u44163\'3F\u52376\'3F\u47100\'3F) \u48512\'3F\u54056\'3F\u54620\'3F \u51060\'3F\u45800\'3F\u51012\'3F \u47932\'3F\u47532\'3F\u52824\'3F\u45716\'3F \u48169\'3F\u48512\'3F\u51228\'3F\u47196\'3F \u44032\'3F\u46301\'3F\u54620\'3F \u51032\'3F\u50896\'3F\u51032\'3F \u44032\'3F\u44172\'3F\u51060\'3F\u47728\'3F, \u48152\'3F\u46976\'3F\u54616\'3F\u47140\'3F\u45716\'3F \u50689\'3F\u46308\'3F\u51012\'3F \u47932\'3F\u47532\'3F\u52824\'3F\u45716\'3F \u50976\'3F\u51061\'3F\u54620\'3F \u48277\'3F\u46308\'3F\u51012\'3F \u45812\'3F\u51008\'3F \u48277\'3F\u51204\'3F(pandect)\u51060\'3F\u47728\'3F, \u49549\'3F\u46108\'3F \u50896\'3F\u47532\'3F(rudiments)\u47484\'3F \u47932\'3F\u47532\'3F\u52824\'3F\u45716\'3F \u44032\'3F\u51109\'3F \u44480\'3F\u51473\'3F\u54620\'3F \u48372\'3F\u49437\'3F\u46308\'3F\u51060\'3F \u45812\'3F\u44596\'3F \u48372\'3F\u47932\'3F \u49345\'3F\u51088\'3F\u51060\'3F\u47728\'3F, \u47560\'3F\u51648\'3F\u47561\'3F\u51004\'3F\u47196\'3F \u50689\'3F\u49373\'3F\u51012\'3F \u54693\'3F\u54644\'3F \u49116\'3F\u50612\'3F\u51256\'3F \u45208\'3F\u50724\'3F\u45716\'3F \u44032\'3F\u51109\'3F \u49692\'3F\u49688\'3F\u54620\'3F \u47932\'3F\u51060\'3F \u45812\'3F\u44596\'3F \u50896\'3F\u52380\'3F(fountain)\u51077\'3F\u45768\'3F\u45796\'3F. }
\par\plain {\f0\fs24\b0\i0 \loch\af0\hich\af0\dbch\af0\uc1\u44536\'3F\u47532\'3F\u44256\'3F \u50612\'3F\u46500\'3F \u51216\'3F\u51060\'3F \u45440\'3F\u46972\'3F\u50872\'3F\u44620\'3F\u50836\'3F? \u50896\'3F\u48376\'3F\u51060\'3F \u46405\'3F\u51060\'3F \u50500\'3F\u45772\'3F \u54616\'3F\u45720\'3F\u47196\'3F\u48512\'3F\u53552\'3F \u50772\'3F\u45796\'3F\u45716\'3F \u51216\'3F\u51060\'3F\u44256\'3F, \u49324\'3F\u46988\'3F\u51060\'3F \u50500\'3F\u45768\'3F\u46972\'3F \u54616\'3F\u45208\'3F\u45784\'3F\u51060\'3F \u51200\'3F\u51088\'3F\u51060\'3F\u49884\'3F\u47728\'3F, \u51089\'3F\u49457\'3F\u51088\'3F(inditer)\u45716\'3F \u49324\'3F\u46020\'3F\u46308\'3F\u51060\'3F\u45208\'3F \u45824\'3F\u50616\'3F\u51088\'3F\u46308\'3F\u51060\'3F \u51088\'3F\u49888\'3F\u51032\'3F \u51648\'3F\u54812\'3F(wit)\u50640\'3F \u46384\'3F\u46972\'3F \u50420\'3F \u44163\'3F\u51060\'3F \u50500\'3F\u45768\'3F\u46972\'3F \u49457\'3F\u47161\'3F\u45784\'3F\u44760\'3F\u49436\'3F \u50416\'3F\u49512\'3F\u45796\'3F\u45716\'3F \u44163\'3F\u51060\'3F\u47728\'3F, \u44592\'3F\u47197\'3F\u51088\'3F\u46308\'3F\u51008\'3F \u47784\'3F\u53468\'3F\u50640\'3F\u49436\'3F\u48512\'3F\u53552\'3F \u44396\'3F\u48324\'3F\u46104\'3F\u50612\'3F \u54616\'3F\u45208\'3F\u45784\'3F\u51032\'3F \u50689\'3F\u51032\'3F \u51473\'3F\u50836\'3F\u54620\'3F \u50689\'3F\u50669\'3F\u51004\'3F\u47196\'3F\u48512\'3F\u53552\'3F \u55192\'3F\u51012\'3F \u48155\'3F\u50520\'3F\u51004\'3F\u47728\'3F, \u51452\'3F\u51228\'3F\u44032\'3F \u51652\'3F\u47532\'3F(verity), \u49888\'3F\u49892\'3F\u54632\'3F(piety), \u49692\'3F\u44208\'3F(purity), \u51032\'3F\u47196\'3F\u50880\'3F(uprightness)\u46972\'3F\u45716\'3F \u44163\'3F\u51060\'3F\u47728\'3F, \u54805\'3F\u49885\'3F\u51008\'3F \u54616\'3F\u45208\'3F\u45784\'3F\u51032\'3F \u47568\'3F\u50432\'3F, \u54616\'3F\u45208\'3F\u45784\'3F\u51032\'3F \u51613\'3F\u50616\'3F(testimony), \u54616\'3F\u45208\'3F\u45784\'3F\u51032\'3F \u50696\'3F\u50616\'3F(oracle), \u51652\'3F\u47532\'3F\u51032\'3F \u47568\'3F\u50432\'3F, \u44396\'3F\u50896\'3F\u51032\'3F \u47568\'3F\u50432\'3F \u46321\'3F\u51060\'3F\u46972\'3F\u45716\'3F \u44163\'3F\u51060\'3F\u47728\'3F, \u44536\'3F \u44208\'3F\u44284\'3F\u45716\'3F \u51060\'3F\u54644\'3F\u51032\'3F \u48731\'3F, \u49444\'3F\u46301\'3F\u51032\'3F \u50504\'3F\u51221\'3F\u49457\'3F(stableness of persuasion), \u51453\'3F\u51008\'3F \u51068\'3F\u46308\'3F\u47196\'3F\u48512\'3F\u53552\'3F\u51032\'3F \u54924\'3F\u44060\'3F, \u49334\'3F\u51032\'3F \u49352\'3F\u47213\'3F\u44172\'3F \u46120\'3F, \u44144\'3F\u47337\'3F\u54632\'3F, \u49457\'3F\u47161\'3F\u45784\'3F \u50504\'3F\u50640\'3F\u49436\'3F\u51032\'3F \u54217\'3F\u44053\'3F\u44284\'3F \u44592\'3F\u49256\'3F\u51060\'3F\u46972\'3F\u45716\'3F \u44163\'3F\u51060\'3F\u47728\'3F, \u47560\'3F\u51648\'3F\u47561\'3F\u51004\'3F\u47196\'3F \u49457\'3F\u44221\'3F\u51012\'3F \u44277\'3F\u48512\'3F\u54632\'3F\u51004\'3F\u47196\'3F\u50024\'3F \u50619\'3F\u45716\'3F \u44208\'3F\u47568\'3F\u44284\'3F \u48372\'3F\u49345\'3F\u51008\'3F \u49457\'3F\u46020\'3F\u46308\'3F\u44284\'3F\u51032\'3F \u44368\'3F\u51228\'3F, \u54616\'3F\u45720\'3F\u51032\'3F \u49457\'3F\u54408\'3F\u50640\'3F \u52280\'3F\u50668\'3F\u54616\'3F\u45716\'3F \u44163\'3F, \u50689\'3F\u50896\'3F\u54616\'3F\u44256\'3F, \u50025\'3F\u51648\'3F \u50506\'3F\u44256\'3F, \u49324\'3F\u46972\'3F\u51648\'3F\u51648\'3F \u50506\'3F\u51012\'3F \u49345\'3F\u49549\'3F\u51032\'3F \u49457\'3F\u52712\'3F\u51077\'3F\u45768\'3F\u45796\'3F. \u49457\'3F\u44221\'3F \u44592\'3F\u47197\'3F\u50640\'3F\u49436\'3F \u51600\'3F\u44144\'3F\u50892\'3F\u54616\'3F\u45716\'3F \u51088\'3F\u47484\'3F \u54665\'3F\u48373\'3F\u54616\'3F\u45796\'3F \u54624\'3F \u44163\'3F\u51060\'3F\u47728\'3F, \u48164\'3F\u45230\'3F\u51004\'3F\u47196\'3F \u47925\'3F\u49345\'3F\u54616\'3F\u45716\'3F \u51088\'3F\u45716\'3F \u49464\'3F \u48176\'3F\u45716\'3F \u45908\'3F \u54665\'3F\u48373\'3F\u54616\'3F\u45796\'3F \u54624\'3F \u44163\'3F\u51077\'3F\u45768\'3F\u45796\'3F. }}