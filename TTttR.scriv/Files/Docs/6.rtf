\pard\plain \fi720\ltrch\loch {\f0\fs24\b0\i0 \loch\af0\hich\af0\dbch\af0\uc1\u49324\'3F\u46988\'3F\u46308\'3F\u51060\'3F \u50612\'3F\u46523\'3F\u44172\'3F \u51088\'3F\u44592\'3F\u44032\'3F \u51060\'3F\u54644\'3F\u54624\'3F \u49688\'3F \u50630\'3F\u45716\'3F \u44163\'3F\u51012\'3F \u47925\'3F\u49345\'3F\u54624\'3F \u49688\'3F \u51080\'3F\u51012\'3F\u44620\'3F\u50836\'3F? \u47784\'3F\u47476\'3F\u45716\'3F \u47568\'3F\u47196\'3F \u49704\'3F\u44200\'3F\u51652\'3F \u44163\'3F\u51012\'3F \u50612\'3F\u46523\'3F\u44172\'3F \u51060\'3F\u54644\'3F\u54624\'3F \u49688\'3F \u51080\'3F\u51012\'3F\u44620\'3F\u50836\'3F? \u49457\'3F\u44221\'3F\u50640\'3F \u44592\'3F\u47197\'3F\u46108\'3F \u44163\'3F\u52376\'3F\u47100\'3F \u8220\'93\u45236\'3F\u44032\'3F \u44536\'3F \u49548\'3F\u47532\'3F\u51032\'3F \u45733\'3F\u47141\'3F\u51012\'3F \u50508\'3F\u51648\'3F \u47803\'3F\u54616\'3F\u47732\'3F \u47568\'3F\u54616\'3F\u45716\'3F \u51088\'3F\u50640\'3F\u44172\'3F \u50556\'3F\u47564\'3F\u51064\'3F\u51060\'3F \u46104\'3F\u44256\'3F \u47568\'3F\u54616\'3F\u45716\'3F \u51088\'3F\u46020\'3F \u45236\'3F\u44172\'3F \u50556\'3F\u47564\'3F\u51064\'3F\u51060\'3F \u46112\'3F\u8221\'94 \u44163\'3F\u51077\'3F\u45768\'3F\u45796\'3F. \u49324\'3F\u46020\'3F\u45716\'3F \u50612\'3F\u46500\'3F \u50616\'3F\u50612\'3F\u46020\'3F \u48176\'3F\u51228\'3F\u54616\'3F\u51648\'3F \u50506\'3F\u50520\'3F\u51004\'3F\u47728\'3F \u55176\'3F\u48652\'3F\u47532\'3F\u51064\'3F\u46308\'3F\u51008\'3F \u44032\'3F\u51109\'3F \u50724\'3F\u47000\'3F\u46108\'3F \u44163\'3F\u51012\'3F, \u44536\'3F\u47532\'3F\u49828\'3F\u51064\'3F\u51008\'3F \u44032\'3F\u51109\'3F \u54413\'3F\u49457\'3F\u54620\'3F(copious) \u44163\'3F\u51012\'3F, \u47196\'3F\u47560\'3F\u51064\'3F(Latin)\u51008\'3F \u44032\'3F\u51109\'3F \u51339\'3F\u51008\'3F \u44163\'3F\u51012\'3F \u48260\'3F\u47532\'3F\u51648\'3F \u50506\'3F\u50520\'3F\u49845\'3F\u45768\'3F\u45796\'3F. \u51088\'3F\u50672\'3F\u51008\'3F \u50864\'3F\u47532\'3F\u44032\'3F \u51060\'3F\u54644\'3F\u54616\'3F\u51648\'3F \u47803\'3F \u54616\'3F\u45716\'3F \u47568\'3F\u51012\'3F \u46307\'3F\u45716\'3F \u54872\'3F\u44221\'3F\u50640\'3F \u51080\'3F\u45716\'3F \u44221\'3F\u50864\'3F \u50864\'3F\u47532\'3F\u45716\'3F \u47749\'3F\u48177\'3F\u55176\'3F \u44480\'3F\u47672\'3F\u44144\'3F\u47532\'3F\u50752\'3F \u44057\'3F\u45796\'3F\u44256\'3F, \u50864\'3F\u47532\'3F\u45716\'3F \u47568\'3F\u54616\'3F\u45716\'3F \u51088\'3F\u46308\'3F\u50640\'3F\u44172\'3F \u46308\'3F\u51012\'3F \u49688\'3F \u50630\'3F\u45716\'3F \u44480\'3F\u47484\'3F \u46028\'3F\u47140\'3F\u45824\'3F\u45716\'3F \u44163\'3F\u51060\'3F\u46972\'3F\u44256\'3F \u51221\'3F\u49345\'3F\u51201\'3F\u51064\'3F \u49324\'3F\u46988\'3F\u51012\'3F \u44032\'3F\u47476\'3F\u52432\'3F \u44256\'3F\u48177\'3F\u54616\'3F\u44172\'3F \u54664\'3F\u49845\'3F\u45768\'3F\u45796\'3F.  Scythian\u51008\'3F \u51060\'3F\u54644\'3F\u54616\'3F\u51648\'3F \u47803\'3F\u54616\'3F\u45716\'3F \u50500\'3F\u53580\'3F\u45348\'3F \u49324\'3F\u46988\'3F\u46308\'3F\u51012\'3F \u50556\'3F\u47564\'3F\u51201\'3F\u51060\'3F\u46972\'3F \u50668\'3F\u44220\'3F\u49845\'3F\u45768\'3F\u45796\'3F. \u44057\'3F\u51008\'3F \u51060\'3F\u50976\'3F\u47196\'3F \u47196\'3F\u47560\'3F \u49324\'3F\u46988\'3F\u46308\'3F\u51008\'3F \u49884\'3F\u47532\'3F\u50500\'3F\u50752\'3F \u50976\'3F\u45824\'3F \u49324\'3F\u46988\'3F\u46308\'3F\u51012\'3F \u44536\'3F\u47111\'3F\u44172\'3F \u49373\'3F\u44033\'3F\u54664\'3F\u44256\'3F, (\u49900\'3F\u51648\'3F\u50612\'3F \u51228\'3F\u47212\'3F\u51312\'3F\u52264\'3F \u55176\'3F\u48652\'3F\u47532\'3F\u47568\'3F\u51012\'3F \u50556\'3F\u47564\'3F\u51201\'3F\u51060\'3F\u46972\'3F \u50668\'3F\u44220\'3F\u45716\'3F\u45936\'3F, \u51060\'3F\u44163\'3F\u51060\'3F \u49688\'3F\u47566\'3F\u51008\'3F \u49324\'3F\u46988\'3F\u50640\'3F\u44172\'3F \u51060\'3F\u49345\'3F\u54664\'3F\u44592\'3F \u46412\'3F\u47928\'3F\u51060\'3F\u50632\'3F\u49845\'3F\u45768\'3F\u45796\'3F) \u53080\'3F\u49828\'3F\u53444\'3F\u54000\'3F\u45432\'3F\u54540\'3F\u51032\'3F \u54889\'3F\u51228\'3F\u46020\'3F \u45768\'3F\u53084\'3F\u46972\'3F\u49828\'3F \u44368\'3F\u54889\'3F\u51060\'3F \u51060\'3F\u50640\'3F \u45824\'3F\u54644\'3F \u48516\'3F\u45432\'3F\u54616\'3F\u44596\'3F \u54664\'3F\u51648\'3F\u47564\'3F \u46972\'3F\u54004\'3F\u50612\'3F\u45716\'3F \u50556\'3F\u47564\'3F\u51201\'3F\u51060\'3F\u46972\'3F\u44256\'3F \u54664\'3F\u49845\'3F\u45768\'3F\u45796\'3F. \u46384\'3F\u46972\'3F\u49436\'3F \u44536\'3F\u47532\'3F\u49828\'3F\u46020\'3F\u51032\'3F \u49884\'3F\u45824\'3F\u48372\'3F\u45796\'3F \u50724\'3F\u47000\'3F \u51204\'3F\u50640\'3F \u49332\'3F\u50520\'3F\u45912\'3F \u50976\'3F\u45824\'3F\u51064\'3F\u46308\'3F\u51008\'3F \u47784\'3F\u46304\'3F \u45796\'3F\u47480\'3F \u45208\'3F\u46972\'3F\u46308\'3F\u51012\'3F Lognazim\u51060\'3F\u46972\'3F \u48520\'3F\u47104\'3F\u45716\'3F\u45936\'3F, \u51060\'3F\u45716\'3F \u50556\'3F\u47564\'3F\u51201\'3F\u51064\'3F \u44163\'3F\u48372\'3F\u45796\'3F \u45208\'3F\u51012\'3F \u44172\'3F \u48324\'3F\u47196\'3F \u50630\'3F\u45796\'3F\u45716\'3F \u46907\'3F\u51077\'3F\u45768\'3F\u45796\'3F. \u44536\'3F\u47000\'3F\u49436\'3F \u45572\'3F\u44400\'3F\u44032\'3F\u45716\'3F \u48520\'3F\u54217\'3F\u54616\'3F\u44592\'3F\u47484\'3F \u47196\'3F\u47560\'3F\u51032\'3F \u50896\'3F\u47196\'3F\u50896\'3F(senate of Rome)\u50640\'3F\u45716\'3F \u53685\'3F\u50669\'3F\u51088\'3F\u47484\'3F \u48512\'3F\u47476\'3F\u45716\'3F \u50612\'3F\u46500\'3F \u49324\'3F\u46988\'3F\u51060\'3F \u54637\'3F\u49345\'3F \u51080\'3F\u50632\'3F\u49845\'3F\u45768\'3F\u45796\'3F. \u46384\'3F\u46972\'3F\u49436\'3F \u44368\'3F\u54924\'3F\u46020\'3F \u44536\'3F\u47088\'3F \u50517\'3F\u48149\'3F(exigent)\u50640\'3F \u51060\'3F\u45132\'3F\u47532\'3F\u51648\'3F \u50506\'3F\u46020\'3F\u47197\'3F \u48264\'3F\u50669\'3F\u48376\'3F\u51012\'3F \u51456\'3F\u48708\'3F\u54644\'3F \u45459\'3F\u45716\'3F \u44163\'3F\u51060\'3F \u54596\'3F\u50836\'3F\u54664\'3F\u49845\'3F\u45768\'3F\u45796\'3F. \u48264\'3F\u50669\'3F\u51008\'3F \u52285\'3F\u51012\'3F \u50676\'3F\u44256\'3F \u48731\'3F\u51060\'3F \u46308\'3F\u50612\'3F\u50724\'3F\u44172\'3F \u54616\'3F\u45716\'3F \u44163\'3F\u51060\'3F\u47728\'3F, \u50864\'3F\u47532\'3F\u44032\'3F \u50508\'3F\u47609\'3F\u51060\'3F\u47484\'3F \u47673\'3F\u51012\'3F \u49688\'3F \u51080\'3F\u46020\'3F\u47197\'3F \u44749\'3F\u45936\'3F\u44592\'3F\u47484\'3F \u48512\'3F\u49688\'3F\u45716\'3F \u44163\'3F\u51060\'3F\u47728\'3F, \u51109\'3F\u47561\'3F\u51012\'3F \u50694\'3F\u51004\'3F\u47196\'3F \u52824\'3F\u50892\'3F\u49436\'3F \u50864\'3F\u47532\'3F\u44032\'3F \u51648\'3F\u49457\'3F\u49548\'3F\u47484\'3F \u48148\'3F\u46972\'3F\u48380\'3F \u49688\'3F \u51080\'3F\u45716\'3F \u44163\'3F\u51060\'3F\u47728\'3F, \u47560\'3F\u52824\'3F \u50556\'3F\u44273\'3F\u51060\'3F \u50864\'3F\u47932\'3F \u51077\'3F\u44396\'3F\u51032\'3F \u46028\'3F\u51012\'3F \u44404\'3F\u47140\'3F\u48372\'3F\u45236\'3F\u49436\'3F \u46972\'3F\u48152\'3F\u51032\'3F \u50577\'3F\u46524\'3F\u44032\'3F \u47932\'3F\u51012\'3F \u47560\'3F\u49892\'3F \u49688\'3F \u51080\'3F\u50632\'3F\u45912\'3F \u44163\'3F\u52376\'3F\u47100\'3F \u50864\'3F\u47932\'3F\u51032\'3F \u46748\'3F\u44753\'3F\u51012\'3F \u50630\'3F\u50528\'3F\u49436\'3F, \u50864\'3F\u47532\'3F\u44032\'3F \u47932\'3F\u47196\'3F \u45796\'3F\u44032\'3F\u44040\'3F \u49688\'3F \u51080\'3F\u44172\'3F \u54644\'3F \u51452\'3F\u45716\'3F \u44163\'3F\u51077\'3F\u45768\'3F\u45796\'3F. \u52280\'3F\u51004\'3F\u47196\'3F \u54217\'3F\u48124\'3F\u46308\'3F\u51032\'3F \u47568\'3F\u47196\'3F \u48264\'3F\u50669\'3F\u46108\'3F \u49457\'3F\u44221\'3F\u51060\'3F \u50630\'3F\u45796\'3F\u47732\'3F, \u48176\'3F\u50864\'3F\u51648\'3F \u47803\'3F\u54620\'3F \u51088\'3F\u46308\'3F\u51008\'3F \u45800\'3F\u51648\'3F \u50556\'3F\u44273\'3F\u51032\'3F (\u44618\'3F\u51008\'3F) \u50864\'3F\u47932\'3F \u44225\'3F\u50640\'3F\u49436\'3F \u47932\'3F\u53685\'3F\u51060\'3F\u45208\'3F \u45796\'3F\u47480\'3F \u44163\'3F\u51060\'3F \u50630\'3F\u50612\'3F\u49436\'3F \u47932\'3F\u51012\'3F \u44600\'3F\u50612\'3F \u50732\'3F\u47540\'3F \u49688\'3F \u50630\'3F\u45716\'3F \u50612\'3F\u47536\'3F \u50500\'3F\u51060\'3F\u46308\'3F \u44057\'3F\u51012\'3F \u44163\'3F\u44144\'3F\u45208\'3F, \u8220\'93\u50896\'3F\u54616\'3F\u44148\'3F\u45824\'3F \u51060\'3F\u44163\'3F\u51012\'3F \u51069\'3F\u51004\'3F\u46972\'3F\u8221\'94\u46972\'3F\u44256\'3F \u48393\'3F\u51064\'3F\u46108\'3F \u52293\'3F\u51060\'3F \u51452\'3F\u50612\'3F\u51276\'3F\u51012\'3F \u46412\'3F \u8220\'93\u44536\'3F\u44163\'3F\u51060\'3F \u48393\'3F\u51064\'3F\u46104\'3F\u50632\'3F\u51004\'3F\u48064\'3F\u47196\'3F \u45236\'3F\u44032\'3F \u51069\'3F\u51012\'3F \u49688\'3F \u50630\'3F\u45432\'3F\u46972\'3F\u8221\'94\u46972\'3F\u44256\'3F \u45824\'3F\u45813\'3F\u54624\'3F \u49688\'3F\u48150\'3F\u50640\'3F \u50630\'3F\u50632\'3F\u45912\'3F \u51060\'3F\u49324\'3F\u50556\'3F\u49436\'3F\u50640\'3F \u44592\'3F\u47197\'3F\u46108\'3F \u54620\'3F \u49324\'3F\u46988\'3F\u52376\'3F\u47100\'3F \u46112\'3F \u44163\'3F\u51077\'3F\u45768\'3F\u45796\'3F. }}